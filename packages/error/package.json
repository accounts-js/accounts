  "license": "MIT",
  "dependencies": {
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "16.4.13",
    "jest": "27.0.6",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4"
  }
