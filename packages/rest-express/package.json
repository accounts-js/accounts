{
  "name": "@accounts/rest-express",
  "version": "0.15.0",
  "description": "Server side REST express middleware for accounts",
  "main": "lib/index",
  "typings": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib",
    "start": "tsc --watch",
    "precompile": "yarn clean",
    "compile": "tsc",
    "prepublishOnly": "yarn compile",
    "test": "npm run testonly",
    "test:watch": "npm run testonly -- --watch --coverage",
    "testonly": "jest",
    "coverage": "npm run testonly -- --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accounts-js/rest/tree/master/packages/rest-express"
  },
  "keywords": [
    "users",
    "accounts",
    "rest",
    "express"
  ],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "devDependencies": {
    "@accounts/server": "^0.15.0",
    "@types/express": "4.17.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "@types/node": "12.6.2",
    "@types/request-ip": "0.0.33",
    "jest": "24.8.0"
  },
  "peerDependencies": {
    "@accounts/server": "^0.7.0"
  },
  "dependencies": {
    "@accounts/types": "^0.15.0",
    "express": "^4.17.0",
    "lodash": "^4.17.15",
    "request-ip": "^2.1.3",
    "tslib": "1.10.0"
  }
}
