{
  "name": "@accounts/rest-express",
  "version": "0.33.1",
  "description": "Server side REST express middleware for accounts",
  "main": "lib/index",
  "typings": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib",
    "start": "tsc --watch",
    "precompile": "pnpm run clean",
    "compile": "tsc",
    "prepublishOnly": "pnpm run compile",
    "test": "npm run testonly",
    "test:watch": "npm run testonly -- --watch --coverage",
    "testonly": "jest",
    "coverage": "npm run testonly -- --coverage"
  },
  "files": [
    "src",
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accounts-js/rest/tree/master/packages/rest-express"
  },
  "keywords": [
    "users",
    "accounts",
    "rest",
    "express"
  ],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "peerDependencies": {
    "@accounts/server": "^0.32.0 || ^0.33.0"
  },
  "dependencies": {
    "@accounts/types": "^0.33.1",
    "express": "4.18.2",
    "request-ip": "3.3.0",
    "tslib": "2.5.2"
  },
  "devDependencies": {
    "@accounts/magic-link": "^0.1.1",
    "@accounts/password": "^0.32.1",
    "@accounts/server": "^0.33.1",
    "@types/express": "4.17.17",
    "@types/jest": "27.5.2",
    "@types/node": "20.2.3",
    "@types/request-ip": "0.0.37",
    "jest": "27.3.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "ts-jest": "27.1.5"
  }
}
