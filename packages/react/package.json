  },
  "devDependencies": {
    "@accounts/client": "^0.4.0-alpha.1",
    "@accounts/client-password": "^0.4.0-alpha.1",
    "@accounts/types": "^0.4.0-alpha.1",
    "@types/jest": "23.3.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "jest": "23.4.2",
    "react": "^16.4.2"
  }
