  "license": "MIT",
  "dependencies": {
    "@accounts/types": "^0.29.0",
    "@types/jsonwebtoken": "8.3.9",
    "emittery": "0.5.1",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.15",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.157",
    "@types/node": "14.0.14",
    "jest": "26.2.2",
    "rimraf": "3.0.2"
  }
