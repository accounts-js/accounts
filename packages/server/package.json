{
  "name": "@accounts/server",
  "version": "0.33.1",
  "description": "Fullstack authentication and accounts-management",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "yarn run -T rimraf lib",
    "start": "yarn run -T tsc --watch",
    "precompile": "yarn run clean",
    "compile": "yarn run -T tsc",
    "prepublishOnly": "yarn run compile",
    "test": "yarn run testonly",
    "test-ci": "yarn run lint && yarn run coverage",
    "testonly": "yarn run -T jest",
    "test:watch": "yarn run -T jest --watch",
    "coverage": "yarn run testonly --coverage"
  },
  "files": [
    "src",
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accounts-js/accounts/tree/master/packages/server"
  },
  "keywords": [
    "rest",
    "graphql",
    "grant",
    "auth",
    "authentication",
    "accounts",
    "users",
    "oauth"
  ],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "^16.0.0",
    "graphql-modules": "^2.0.0"
  },
  "dependencies": {
    "@accounts/types": "^0.33.1",
    "@types/jsonwebtoken": "9.0.2",
    "emittery": "0.13.1",
    "jsonwebtoken": "9.0.0",
    "jwt-decode": "3.1.2",
    "lodash.merge": "4.6.2",
    "tslib": "2.5.2"
  },
  "devDependencies": {
    "@types/lodash.merge": "4.6.7",
    "graphql": "16.8.0",
    "graphql-modules": "2.2.0",
    "reflect-metadata": "0.1.13"
  }
}
