{
  "name": "@accounts/graphql-client",
  "version": "0.10.0",
  "description": "GraphQL client transport for accounts",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "start": "tsc --watch",
    "precompile": "yarn clean",
    "compile": "tsc",
    "prepublishOnly": "yarn compile"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-accounts/graphql.git"
  },
  "author": "David Yahalomi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-accounts/graphql/issues"
  },
  "homepage": "https://github.com/js-accounts/graphql#readme",
  "devDependencies": {
    "lodash": "4.17.11",
    "nock": "10.0.6"
  },
  "dependencies": {
    "@accounts/client": "^0.10.0",
    "@accounts/types": "^0.10.0",
    "graphql-tag": "^2.10.0",
    "tslib": "1.9.3"
  }
}
